<?xml version="1.0" encoding="UTF-8"?>

<processors xmlns="http://mokai.googlecode.com/processors" 
		    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		    xsi:schemaLocation=
		   		"http://mokai.googlecode.com/processors http://mokai.googlecode.com/svn/schema/processors-0.1.xsd">
		   		
  <processor id="comcel" priority="1000" className="org.mokai.connector.smpp.SmppConnector">
  	<configuration>
  		<property name="host">localhost</property>
  		<property name="port">8321</property>
  		<property name="systemId">test</property>
  		<property name="password">test</property>
  	</configuration>
    <acceptors>
      <acceptor className="org.mokai.acceptor.RegExpAcceptor">
        <property name="field">to</property>
        <property name="regexp">(57)?(300)(\d){7}</property>
      </acceptor>
    </acceptors>
    
  </processor>
</processors>
