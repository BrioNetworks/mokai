<?xml version="1.0" encoding="UTF-8"?>

<processors xmlns="http://mokai.googlecode.com/processors" 
		    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		    xsi:schemaLocation=
		   		"http://mokai.googlecode.com/processors http://mokai.googlecode.com/svn/schema/processors-0.1.xsd">
		   		
  <processor id="mokai-test" priority="700" className="org.mokai.connector.smpp.SmppConnector">
  	<configuration>
  		<property name="host">localhost</property>
  		<property name="port">8321</property>
  		<property name="systemId">test</property>
  		<property name="password">test</property>
  	</configuration>
    <acceptors>
      <acceptor className="org.mokai.acceptor.AcceptAllAcceptor" />
    </acceptors>
  </processor>
  
  <processor id="comcel" priority="1000" className="org.mokai.connector.http.HttpConnector">
  	<configuration>
  		<property name="url">http://www.google.com/</property>
  	</configuration>
  	<acceptors>
      <acceptor className="org.mokai.acceptor.AcceptAllAcceptor" />
    </acceptors>
  </processor>
</processors>
